# Treat model and data files as large binary files in Git
*.h5 filter=lfs diff=lfs merge=lfs -text
*.pkl filter=lfs diff=lfs merge=lfs -text

# Optional: avoid tracking temporary or system files
*.pyc export-ignore
__pycache__/ export-ignore
.env export-ignore
.DS_Store export-ignore
Riley_AI_Core.zip filter=lfs diff=lfs merge=lfs -text
